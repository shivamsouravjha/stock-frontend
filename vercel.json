{
  "headers": [
    {
      "source": "/.well-known/funding-manifest-urls",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/.well-known/funding-manifest-urls",
      "destination": "/public/.well-known/funding-manifest-urls"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}